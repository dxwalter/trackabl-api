{
  "name": "wellatmarket",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:prod": "NODE_ENV=production sequelize-cli db:migrate",
    "migrate:dev:up": "sequelize-cli db:migrate",
    "migrate:dev:down": "sequelize-cli db:migrate:undo:all",
    "migrate:test": "NODE_ENV=test sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e:husky": "set NOCK_TEST=true && cross-env NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && jest --watchAll",
    "test:cov": "cross-env NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && jest --coverage",
    "test:debug": "cross-env NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:nock": "set NOCK_TEST=true && cross-env DEBUG=nock.* NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/nock-setup.sh && jest --config ./test/jest-e2e.json --watchAll --detectOpenHandles",
    "test:e2e": "set NOCK_TEST= && cross-env DEBUG=nock.* NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && jest --config ./test/jest-e2e.json --watchAll --detectOpenHandles",
    "test:e2e:specific": "set NOCK_TEST= && cross-env DEBUG=nock.* NODE_ENV=test bash ./test/setup/destroy.sh && cross-env NODE_ENV=test bash ./test/setup/setup.sh && jest --config ./test/jest-e2e-specific.json --watchAll --detectOpenHandles",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.0",
    "aws-sdk": "^2.1480.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.2.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "disposable-email-domains": "^1.0.62",
    "dotenv": "^16.3.1",
    "g": "^2.0.1",
    "logrocket": "^7.0.0",
    "moesif-nodejs": "^3.6.1",
    "mysql2": "^3.6.1",
    "newrelic": "^11.13.0",
    "nock": "^13.5.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "random-email": "^1.0.3",
    "randomstring": "^1.3.0",
    "referral-code-generator": "^1.0.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.5",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.17",
    "umzug": "^3.3.1",
    "voucher-code-generator": "^1.3.0",
    "zeptomail": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
